<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Other_match_item">

  <ImageView
      android:id="@+id/icon_champ"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      tools:src="@tools:sample/avatars" />

  <ImageView
      android:id="@+id/summoner1"
      android:layout_width="69dp"
      android:layout_height="67dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/icon_champ"
      tools:src="@tools:sample/avatars" />

  <ImageView
      android:id="@+id/summoner2"
      android:layout_width="69dp"
      android:layout_height="67dp"
      app:layout_constraintStart_toEndOf="@+id/summoner1"
      app:layout_constraintTop_toBottomOf="@+id/icon_champ"
      tools:src="@tools:sample/avatars" />

  <TextView
      android:id="@+id/gamemode"
      android:layout_width="128dp"
      android:layout_height="22dp"
      android:layout_marginStart="40dp"
      android:layout_marginTop="5dp"
      android:text="TextView"
      app:layout_constraintStart_toEndOf="@+id/icon_champ"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/dateGame"
      android:layout_width="125dp"
      android:layout_height="22dp"
      android:layout_marginTop="20dp"
      android:text="TextView"
      app:layout_constraintStart_toStartOf="@+id/gamemode"
      app:layout_constraintTop_toBottomOf="@+id/gamemode" />

  <TextView
      android:id="@+id/timeGame"
      android:layout_width="88dp"
      android:layout_height="22dp"
      android:layout_marginStart="10dp"
      android:text="TextView"
      app:layout_constraintStart_toEndOf="@+id/dateGame"
      app:layout_constraintTop_toTopOf="@+id/dateGame" />

  <ListView
      android:id="@+id/listItems"
      android:layout_width="412dp"
      android:layout_height="117dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/listRunes" />

  <ListView
      android:id="@+id/listMatchmaking"
      android:layout_width="412dp"
      android:layout_height="127dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/listItems" />

  <ListView
      android:id="@+id/listRunes"
      android:layout_width="273dp"
      android:layout_height="64dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/summoner2"
      app:layout_constraintTop_toBottomOf="@+id/kda" />

  <TextView
      android:id="@+id/kda"
      android:layout_width="216dp"
      android:layout_height="23dp"
      android:layout_marginTop="10dp"
      android:text="TextView"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="@+id/dateGame"
      app:layout_constraintTop_toBottomOf="@+id/dateGame" />

</androidx.constraintlayout.widget.ConstraintLayout>